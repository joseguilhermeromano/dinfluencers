{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["dom", "esnext"],
    "moduleResolution": "Node",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true
  },
  "include": ["resources/js"]
}
